{
  "name": "validator",
  "version": "1.0.0",
  "private": true,
  "description": "Nat's Project Validator",
  "author": "Nat Welch <nat@natwelch.com>",
  "license": "ISC",
  "repository": "https://github.com/icco/validator.git",
  "homepage": "https://github.com/icco/validator",
  "bugs": "https://github.com/icco/validator/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^10.0.0",
    "probot-scheduler": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "jest": ">= 24.9.0",
    "nock": "^13.0.6",
    "prettier": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^16.0.0"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "snyk": true,
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
